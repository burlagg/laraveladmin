{
	"name": "burlagg/laravel-administrator",
	"description": "Fork of Exodusanto (frozennode) Laravel Administrator. Supports newer versions of Laravel",
	"homepage": "https://github.com/burlagg/laraveladmin",
	"keywords": ["administrator", "admin", "database", "laravel-administrator", "laravel", "cms", "frozennode", "exodusanto"],
	"license": "MIT",
	"authors": [
		{
			"name": "Jan Hartigan",
			"email": "jan@frozennode.com"
		},
		{
			"name": "Antonio Dal Sie",
			"email": "info@antoniodalsie.com"
		}
	],
	"require": {
		"php": "^8.2",
		"laravel/framework": "^11.0",
		"ckeditor/ckeditor": "4.*",
		"mockery/mockery": "^1.5"
	},
	"require-dev": {
        "phpunit/phpunit": "^10.0"
	},
	"autoload": {
		"classmap": [
			"src/controllers"
		],
		"psr-0": {
			"Frozennode\\Administrator": "src/"
		}
	},
	"extra": {
	    "laravel": {
	        "providers": [
	            "Frozennode\\Administrator\\AdministratorServiceProvider"
	        ]
	    }
	},
	"scripts": {
		"post-update-cmd": [
			"if [ -d \"public/js/ckeditor/\" ] \nthen \nrm -R public/js/ckeditor/ \nfi ",
			"if [ -d \"vendor/ckeditor/ckeditor/\" ] \nthen \nmv vendor/ckeditor/ckeditor/ public/js/  \nfi",
			"if [ -d \"vendor/ckeditor/\" ] \nthen \nrm -R vendor/ckeditor/ \nfi"
		]
	},
	"prefer-stable": true
}
